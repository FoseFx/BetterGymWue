{
  "name": "v1-x",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "npm run build && npm run after_build",
    "build:test": "ng build && npm run prod_replacer_script && npm run sw_precache && http-server-spa ./dist index.html",
    "sw_precache": "sw-precache --root=dist --config=precache-config.js",
    "minify": "uglifyjs-folder dist -e -y -x .js -o dist",
    "prod_replacer_script": "replacer dist/index.html \"prod = false\" \"prod = true\"",
    "after_build": "npm run prod_replacer_script && copymycsstomyhtml && npm run sw_precache && npm run minify",
    "server": "http-server-spa ./dist index.html",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.5",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "^6.0.5",
    "@angular/compiler": "^6.0.5",
    "@angular/core": "^6.0.5",
    "@angular/forms": "^6.0.5",
    "@angular/http": "^6.0.5",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.5",
    "@angular/platform-browser-dynamic": "^6.0.5",
    "@angular/router": "^6.0.5",
    "@types/hammerjs": "^2.0.35",
    "core-js": "^2.5.7",
    "firebase": "^5.5.1",
    "hammerjs": "^2.0.8",
    "npm": "^5.10.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/compiler-cli": "^6.0.5",
    "@angular/language-service": "^6.0.5",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.113",
    "codelyzer": "^4.3.0",
    "copymycsstomyhtml": "file:../copymycsstomyhtml",
    "firebase-tools": "^4.2.1",
    "http-server-spa": "^1.3.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "replacer": "file:../replacer",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "uglifyjs-folder": "^1.5.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "wildcardify": "file:../wildcardify"
  }
}
